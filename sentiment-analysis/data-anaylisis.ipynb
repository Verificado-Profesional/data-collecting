    "import pandas as pd\n",
    "import xml.etree.ElementTree as ET"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Parse XML file\n",
    "def parse_xml(filename):\n",
    "    tree = ET.parse(filename)\n",
    "    return tree.getroot()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "def create_dataframe(filename):\n",
    "    root = parse_xml(filename)\n",
    "    columns = []\n",
    "    datos = []\n",
    "    \n",
    "    for child in root:\n",
    "        if not columns:\n",
    "            columns = [element.tag for element in child]\n",
    "        fila = [element.text for element in child]\n",
    "        datos.append(fila)\n",
    "\n",
    "    df = pd.DataFrame(datos, columns=columns)\n",
    "\n",
    "    return df "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 123,
   "metadata": {},
   "outputs": [],
   "source": [
    "def define_polarity(polarities):\n",
    "    aux_lst =[]\n",
    "    for polarity in polarities.findall('polarity'):\n",
    "         if polarity.find('value') is not None:\n",
    "          aux_lst.append(polarity.find('value').text)\n",
    "         else:\n",
    "            aux_lst(None)\n",
    "    if 'N' in aux_lst and 'P' in aux_lst:\n",
    "        return [None]\n",
    "    if 'NEU' in aux_lst and 'N' in aux_lst and 'P' in aux_lst:\n",
    "      aux_lst.remove('NEU')\n",
    "      return aux_lst\n",
    "    if  aux_lst[:-1] == aux_lst[1:]:\n",
    "       return [aux_lst[0]]\n",
    "    return aux_lst"
   ]
